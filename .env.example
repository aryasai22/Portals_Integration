# Copy this to .env and fill in your credentials
# Don't commit .env to git

# CEIPAL API credentials
CEIPAL_BASE_URL=https://api.ceipal.com
CEIPAL_EMAIL=your.email@company.com
CEIPAL_PASSWORD=your_password
CEIPAL_API_KEY=your_api_key

# Snowflake (optional - leave blank for JSON-only testing)
SNOWFLAKE_ACCOUNT=
SNOWFLAKE_USER=
SNOWFLAKE_PASSWORD=
SNOWFLAKE_WAREHOUSE=COMPUTE_WH
SNOWFLAKE_DATABASE=PORTALS_DW
SNOWFLAKE_ROLE=SYSADMIN

# Date range - only needed for expenses/invoices (max 90 days)
CEIPAL_FROM_DATE=2025-07-25
CEIPAL_TO_DATE=2025-10-23

# Performance tuning (optional)
CEIPAL_DETAIL_RPS=2.0
CEIPAL_DETAIL_WORKERS=1

